@using EmployeeInformations.Model.PrivilegeViewModel;
@model RoleViewModel

<div id="bind-dashboardrolemodal-content">
</div>

<input type="hidden" id="txt-RoletId" value="@Model.RoleId" />


<script type="text/javascript">

    $(document).ready(function () {
        var roleId = $("#txt-RoletId").val();
        PageUtil.goToDashboardPermission("/Permission/UpdateDashboardRolePermission?roleId=" + roleId);
    });

</script>