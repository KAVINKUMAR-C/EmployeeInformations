                width: 0;
                height: 0;
            }

        .slider__Custom {
            position: absolute;
            cursor: pointer;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background-color: #ccc;
            transition: .4s;
            border-radius: 34px;
        }

            .slider__Custom:before {
                position: absolute;
                content: "";
                height: 14px;
                width: 14px;
                left: 3px;
                bottom: 3px;
                background-color: white;
                transition: .4s;
                border-radius: 50%;
            }

        .switch__Custom input:checked + .slider__Custom {
            background-color: #1a76d1;
        }

            .switch__Custom input:checked + .slider__Custom:before {
                transform: translateX(14px);
            }

