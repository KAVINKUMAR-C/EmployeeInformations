@using Microsoft.AspNetCore.Http;
@using EmployeeInformations.Business.IService
@using EmployeeInformations.Common.Enums
@inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor
@inject Microsoft.Extensions.Configuration.IConfiguration configuration
@inject IEmployeeInformationService employeeInformationService
@{
    var userName = Context.Session.GetString("UserName");
    var employeeName = Context.Session.GetString("EmployeeName");
    var loginProfileImage = Context.Session.GetString("ProfileName");
    var roleId = Context.Session.GetInt32("RoleId");
    var empId = Context.Session.GetInt32("EmpId");
    var companyId = Context.Session.GetInt32("CompanyId");
    var displayId = Context.Session.GetInt32("DisplayId");
    var isOnboarding = Context.Session.GetInt32("IsOnboarding");   
    

    var RolePrivileges = new List<int>();

    if (roleId >= 0)
    {
        var id = roleId.Value;
        RolePrivileges = await employeeInformationService.LayoutRolePermissionByRoleId(id);
    }
    if (companyId >= 0)
    {
        displayId = await employeeInformationService.LayoutDisplayMode(companyId.Value);
    }
}

