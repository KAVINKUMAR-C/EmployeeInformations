<script type="text/javascript">
        // Apply theme immediately to prevent flickering
    @* (function () {
      // Remove existing theme classes
      document.body.classList.remove("dark-mode", "light-mode");

      // Get the theme from localStorage
      var theme = localStorage.getItem('theme') || 'light'; // Default to 'light' if no theme is set

      // Apply the theme
      if (theme === 'dark') {
        document.body.classList.add('dark-mode');
        $('.theme-mode')[0].href = "./assets/css/dark-mode.css?ver=3.1.1";
      } else {
        document.body.classList.add('light-mode');
        $('.theme-mode')[0].href = "./assets/css/light-mode.css?ver=3.1.1";
      }

      // Set the toggle state
      $("#darkModeToggle").prop('checked', theme === 'dark');
    })();

    // Event listener for the dark mode toggle
    $('#darkModeToggle').on('change', function () {
      // Toggle the theme classes
      document.body.classList.toggle("dark-mode");
      document.body.classList.toggle("light-mode");

      // Update the theme in localStorage and the CSS file
      var theme = localStorage.getItem('theme');
      if (theme === 'dark') {
        localStorage.setItem('theme', 'light');
        $('.theme-mode')[0].href = "./assets/css/light-mode.css?ver=3.1.1";
      } else {
        localStorage.setItem('theme', 'dark');
        $('.theme-mode')[0].href = "./assets/css/dark-mode.css?ver=3.1.1";
      }
    }); *@
        function getTheme() {
        var theme = localStorage.getItem('theme') || 'light'; // Default to light mode
        document.documentElement.setAttribute("data-theme", theme);
        $('.theme-mode')[0].href = `./assets/css/${theme}-mode.css?ver=3.1.1`;

        // Ensure toggle reflects current mode
        $("#darkModeToggle").prop('checked', theme === 'dark');
    }

    $(document).ready(function () {
        getTheme();

        $('#darkModeToggle').on('change', function () {
            var newTheme = this.checked ? 'dark' : 'light';
            localStorage.setItem('theme', newTheme);
            document.documentElement.setAttribute("data-theme", newTheme);
            $('.theme-mode')[0].href = `./assets/css/${newTheme}-mode.css?ver=3.1.1`;
        });
    });


    // Other document.ready logic (if needed)
    $(document).ready(function () {
      $('#success_Reject_Popup').modal({
        backdrop: 'static',
        keyboard: false
      });
    });
</script>
