<script>
    $(document).ready(function () {
        var count = 1;
        var columnIndex = "";
        var columnDirection = "";
        var entries = 10;
        getWfhDatas();

        function getWfhDatas() {
            var table = $('#getLeaveData').DataTable();
            if (table) {
                table.destroy();
            }
            $('#getLeaveData').DataTable({
                // processing: true,
                serverSide: true,
                processing: true,
                pageLength: entries, // Page length
                ajax: {
                    url: "/WorkFromHome/WorkFromHomeFilter",
                    type: "GET",
                    data: function (d) {
                        //SearchFilter
                        var wfhFilter = document.getElementById("getLeaveData_filter");
                        var wfhFilterSearch = wfhFilter.querySelector("input[type='search']");
                        var wfhFilterinput = wfhFilterSearch.value;
                        //PagingLength
                        var wfhFilterData = document.getElementById("getLeaveData_length");
                        var page = wfhFilterData.querySelector("select");
                        var wfhFilterCount = page.value;
                      
                        d.sSearch = wfhFilterinput;
                        d.iDisplayStart = d.iDisplayStart || 0;
                        d.iDisplayLength = d.iDisplayLength || wfhFilterCount;
                        d.sEcho = d.start;
                        d.columnName = columnIndex;
                        d.columnDirection = columnDirection;
                    }
                },
                columns: [
                    {
                        data: 'employeeName',
                        render: function (data, type, row) {
                            if (count > 5)
                                count = 1;
                            var className = GetClassNameForLeaveDashboard(count);
                            count++;

                            return "<div class='user-card'>" +
                                (row.profileFilePath == "" ?
                                    "<div class='user-avatar " + className + "'><span style='text-transform:uppercase'>" + (row.employeeName)[0] + "</span></div>" :
                                    "<div class='user-avatar bg-light'><img src='./ProfileImages/" + row.profileFilePath + "' alt=''></div>") +
                                "<div class='user-info'>" +
                                "<div class='user-info'>" + (row.employeeStatus == true ? "<a href='javascript:void(0)' id='viewEmployee' data-empid=" + row.empId + "><span class='tb-lead text-danger'>" + row.employeeName + "</span></a>" : "<a href='javascript:void(0)' id='viewEmployee' data-empid=" + row.empId + "><span class='tb-lead font-weight-bold'>" + row.employeeName + "</span></a>") + "</div><span class='tb-sub text-primary'>" + row.userName + "</span></div></div>";

                        }
                    },

                    {
                        data: 'leaveFromDate',
                        render: function (data, type, row) {
                            if (row.id == 5) {
                                return "<span>" + dateFormat(row.leaveFromDate) + "</span>";
                            }
                            else {
                                return "<span>" + dateFormat(row.leaveFromDate) + "</span>";
                            }
                        }
                    },

                    {
                        data: 'leaveToDate',
                        render: function (data, type, row) {
                            if (row.id == 5) {
                                return "<span>" + dateFormat(row.leaveToDate) + "</span>";
                            }
                            else {
                                return "<span>" + dateFormat(row.leaveToDate) + "</span>";
                            }
                        }
                    },

                    {
                        data: 'leaveApplied',
                        render: function (data, type, row) {
                            var leaveValue = parseFloat(row.leaveApplied).toFixed(1); // Formats to two decimal places
                            return "<span>" + leaveValue + "</span>";

                        }
                    },
                    {
                        data: 'reason',
                        render: function (data, type, row) {
                            if (row.reason == null) { row.reason = "" } else { var trimNewValue = row.reason.replace(/'/g, "").trim(); }
                            return " <div class='leave-reason'><p type = 'button' class='tb-sub font-weight-bold' data-bs-toggle='tooltip' data-bs-placement='bottom' title = '" + trimNewValue + "'>" + row.reason + "</p></div>"

                        }
                    },
                    {
                        data: 'leaveStatus',
                        render: function (data, type, row) {
                            if (row.isApproved == 1) {
                                return " <b class='badge badge-sm badge-dot has-bg bg-success d-sm-inline-flex'>Confirmed</b>"
                            }
                            else if (row.isApproved == 0) {
                                return "<b class='badge badge-sm badge-dot has-bg bg-warning d-sm-inline-flex'>Pending</b>"
                            }
                            else if (row.isApproved == 2) {
                                return "<b class='badge badge-sm badge-dot has-bg bg-danger d-sm-inline-flex'>Rejected</b>"
                            }

                        }
                    },
                    {
                        data: 'null',
                        "orderable": false,
                        render: function (data, type, row) {

                            var data = "<div class='nk -tb-col nk-tb-col-tools py-0 border-0'><ul class='nk-tb-actions gx-1 my-n1'><li class='me-n1'><div class='dropdown'><a href='#' class='dropdown-toggle btn btn-icon btn-trigger' data-bs-toggle='dropdown'><em class='icon ni ni-more-h'></em></a><div class='dropdown-menu dropdown-menu-end'><ul class='link-list-opt no-bdr'><li><a href='javascript:void(0)' id = 'btnViewLeaveDetail' data-empid=" + row.empId + " data-id=" + row.appliedLeaveId + " data-empname=" + row.employeeName + "><em class='icon ni ni-eye'></em><span>View</span></a></li>"

                            if (@loginRoleId == 1 || @loginRoleId == 2) {
                                if (row.leaveName != null) {
                                    data += "<li><a href='javascript: void (0)' class='btnDownload' data-id= '" + row.appliedLeaveId + "' ><em class='icon ni ni-download'></em><span>Download</span></a></li>"
                                }

                                if (row.isApproved == 0) {
                                    data += "<li><a href='javascript: void (0)' id='btnLeaveApproved' data-empid=" + row.empId + " data-id=" + row.appliedLeaveId + " data-empname=" + row.employeeName + "><em class='icon ni ni-check'></em><span>Approve</span></a></li>" +
                                        "<li><a href='javascript: void (0)' id='rejectLeaveBtn' data-empids=" + row.empId + " data-ids=" + row.appliedLeaveId + " data-empnames=" + row.employeeName + "><em class='icon ni ni-cross'></em><span>Reject</span></a></li>"
                                }
                            }
                            else {
                                if (row.empId == @loginEmpId) {
                                    if (row.isApproved == 0) {

                                        data += "<li><a href='javascript:void(0)' id='editLeaveBtn' data-id=" + row.appliedLeaveId + " data-isconfirm=" + row.isApproved + "><em class='icon ni ni-edit'></em><span>Edit</span ></a></li>"

                                        if (row.leaveName != null) {
                                            data += "<li><a href ='javascript:void(0)' class='btnDownload' data-id= '" + row.appliedLeaveId + "' ><em class='icon ni ni-download'></em><span>Download</span></a></li>"
                                        }
                                    }
                                    else {
                                        if (row.leaveName != null) {
                                            data += "<li><a href ='javascript:void(0)' class='btnDownload' data-id= '" + row.appliedLeaveId + "' ><em class='icon ni ni-download'></em><span>Download</span></a></li>"
                                        }
                                    }
                                }
                                else {
                                    if (row.isApproved == 0) {
                                        data += "<li><a href='javascript:void(0)' id='btnLeaveApproved' data-empid=" + row.empId + " data-id= " + row.appliedLeaveId + " data-empname= " + row.employeeName + "><em class='icon ni ni-check'></em><span>Approve</span></a></li>" +
                                            "<li><a href='javascript:void(0)' id='rejectLeaveBtn' data-empids=" + row.empId + " data-ids= " + row.appliedLeaveId + " data-empnames= " + row.employeeName + "><em class='icon ni ni-cross'></em><span>Reject</span></a></li>"
                                    }
                                    if (row.leaveName != null) {
                                        data += "<li><a href='javascript: void (0)' class='btnDownload' data-id= '" + row.appliedLeaveId + "' ><em class='icon ni ni-download'></em><span>Download</span></a></li>"
                                    }
                                    else {
                                        if (row.leaveName != null) {
                                            data += "<li><a href = 'javascript: void (0)' class='btnDownload' data-id= '" + row.appliedLeaveId + "' ><em class='icon ni ni-download'></em><span>Download</span></a></li>"
                                        }

                                    }
                                }
                            }
                            data += "</ul></div></div></li></ul></div>";
                            return data;
                        }
                    },
                ],
               
                "createdRow": function (row, data) {
                    // Add custom class to the row
                    $(row).addClass('nk-tb-item');
                    // Add custom classes to each cell in the row if necessary
                    $(row).find('td').addClass('nk-tb-col tb-col');
                }
            });
            // $('[data-bs-toggle="tooltip"]').tooltip();
            $("body").tooltip({ selector: '[data-bs-toggle=tooltip]' });
        }
       
        $(document).on('click', '.sorting, .sorting_asc, .sorting_desc', function (e) {
            var target = e.target;
            var parent = target.parentNode;
            var index = [].indexOf.call(parent.children, target);

            if ($(this).hasClass('sorting_asc')) {
                columnDirection = 'desc';
            }
            else {
                columnDirection = 'asc'; // Assuming default sorting direction is descending
            }

            columnIndex = $(this).data('id');
        });
    });

    function GetClassNameForLeaveDashboard(input) {
        var className = "";
        switch (input) {
            case 1:
                className = "bg-info";
                break;
            case 2:
                className = "bg-pink";
                break;
            case 3:
                className = "bg-success";
                break;
            case 4:
                className = "bg-blue";
                break;
            case 5:
                className = "bg-purple";
                break;
            //case 6:
            //    className = "bg-danger";
            //    break;
        }
        return className;
    }
    function LeavetypeStatus() {

        var CasualLeave = 1;
        var SickLeave = 2;
        var EarnedLeave = 3;
        var MaternityLeave = 4;
        var Permission = 5;
        var LOP = 6;
        var WorkFromHome = 7;
        var CompensatoryOff = 8;
    }
    function dateFormat(leaveFromDate) {

        var dateObject = new Date(leaveFromDate);
        var day = ("0" + dateObject.getDate()).slice(-2);
        var month = ("0" + (dateObject.getMonth() + 1)).slice(-2);
        var year = dateObject.getFullYear();
        var formattedDate = day + "/" + month + "/" + year;
        return formattedDate;
    }
