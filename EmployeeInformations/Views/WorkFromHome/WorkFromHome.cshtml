    //------------------------------------------------------------------------------------------------------------------------------------

    // Delete work from home

    $('#getLeaveData').on('click', '#btnLeaveDelete', function () {
        Swal.fire({
            title: 'Are you sure want to delete?',
            // text: "You won't be able to revert this!",
            icon: 'warning',
            showCancelButton: true,
            confirmButtonColor: '#1ee0ac',
            cancelButtonColor: '#d33',
            confirmButtonText: 'Yes',
            cancelButtonText: 'No',
            allowOutsideClick: false
        }).then((result) => {
            if (result.isConfirmed) {

                var id = $(this).data("id")
                var empid = $(this).data("empid")
                var leave = {
                    appliedLeaveId: id,
                    empId: empid
                };
                $('.loader').removeClass('d-none');
                $.ajax({
                    type: "POST",
                    url: "/WorkFromHome/DeleteWorkFromHome",
                    data: leave,
                    success: (Result) => {

                        if (Result = "Success") {
                            $("#success_Delete_Popup").modal('show');
                            $("#btnCloseDelete").click(function () {
                                var urlToRedirect = '@Url.Action("WorkFromHome", "WorkFromHome")';
                                window.location.href = urlToRedirect;
                            });
                        }

                        $('.loader').addClass('d-none');
                    },
                    error: (Result) => {
                        $('.loader').addClass('d-none');
                    }
                });
            }
        });
    });
