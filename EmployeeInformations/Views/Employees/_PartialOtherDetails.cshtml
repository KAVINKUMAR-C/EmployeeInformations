                    data: otherDetails,
                    success: (result) => {
                        $('#displayotherdetails').html("");
                        $("#success_DeleteOtherDetails_Popup").css('display', 'none !important');
                        Swal.fire({
                            title: 'Deleted Successfully!',
                            text: "",
                            icon: 'success',
                            allowOutsideClick: false
                        }).then((resultValue) => {
                            if (resultValue.isConfirmed) {
                                if (result.otherDetails.length == 0) {
                                    $('#displayotherdetails').append("<tr><td class='dataTables_empty' style = 'text-align:center;' colspan = '6' > No data available </td></tr>");
                                }
                                else {
                                    $.each(result.otherDetails, function (key, value) {
                                        if (value.validFrom != null) {
                                            var obj = new Date(value.validFrom);
                                            var yyyy = obj.getFullYear();
                                            var mm = obj.getMonth() + 1;
                                            var dd = obj.getDate();
                                            if (dd < 10) dd = '0' + dd;
                                            if (mm < 10) mm = '0' + mm;
                                            var validFrom = dd + '/' + mm + '/' + yyyy;

                                            var valueValidTo = new Date(value.validTo);
                                            var yyyy = valueValidTo.getFullYear();
                                            var mm = valueValidTo.getMonth() + 1;
                                            var dd = valueValidTo.getDate();
                                            if (dd < 10) dd = '0' + dd;
                                            if (mm < 10) mm = '0' + mm;
                                            var validTo = dd + '/' + mm + '/' + yyyy;
                                        }
                                        else if (value.validTo != null && value.documentTypeId == 4) {
                                            var validFrom = "";
                                            var valueValidTo = new Date(value.validTo);
                                            var yyyy = valueValidTo.getFullYear();
                                            var mm = valueValidTo.getMonth() + 1;
                                            var dd = valueValidTo.getDate();
                                            if (dd < 10) dd = '0' + dd;
                                            if (mm < 10) mm = '0' + mm;
                                            var validTo = dd + '/' + mm + '/' + yyyy;
                                        }
                                        else {
                                            var validFrom = '';
                                            var validTo = '';
                                        }

                                        $('#displayotherdetails').append("<tr><td> " + value.documentTypeName + "</td><td> " + value.documentNumber + "</td><td style='display:none;'> " + value.documentName + "</td><td> " + validFrom + " </td><td> " + validTo + " </td><td><div class='nk-tb-col nk-tb-col-tools py-0 border-0'><ul class= 'nk-tb-actions gx-1 my-n1' ><li class='me-n1' ><div class='dropdown' ><a href='#' class='dropdown-toggle btn btn-icon btn-trigger' data-bs-toggle='dropdown' > <em class='icon ni ni-more-h' > </em></a ><div class='dropdown-menu dropdown-menu-end' ><ul class='link-list-opt no-bdr' ><li><a href='javascript:void(0)' id='EditBtn' data-id='" + value.detailId + "'data-uan='" + value.uanNumber + "'> <em class='icon ni ni-edit' > </em><span>Edit</span > </a></li ><li><a href='javascript:void(0)' id='btnDelete' data-id='" + value.detailId + "' > <em class='icon ni ni-delete' > </em><span>Delete</span > </a></li ><li><a href='javascript:void(0)' id='btnDownload' data-id='" + value.detailId + "' > <em class='icon ni ni-download' > </em><span>Download</span > </a></li ></ul></div></div></li></ul></div></td></tr>");
                                    });
                                }
                                $('.loader').addClass('d-none');
                            };
                        });                       
                    },
                    complete: function () {
                        $('.loader').addClass('d-none');
                    }
                });
                $("#DocumentTypeId option").removeAttr('selected', 'selected');
                $("#DocumentTypeId option[value='']").attr('selected', 'selected');
                $("#documentNumber").val('');
                $("#DocumentName").val('');
                $("#displayDocumentName").text('');
                $("#btnUpdate").hide();
                $("#btnAdd").show();
                $("#errValid-DocumentTypeId").hide();
            }
        })
    });

    $("#DocumentTypeId").change(function () {
        var selectedText = $(this).find("option:selected").text();
        var selectedValue = $(this).val();
        if (selectedText == 'Passport') {
            $("#validFrom").show();
            $("#validTo").show();

        }
        else if (selectedText == 'DrivingLicence') {
            $("#validFrom").hide();
            $("#validTo").show();
            $("#ValidFrom").val('');
        }
        else {
            $("#validFrom").hide();
            $("#validTo").hide();
            $("#ValidFrom").val('');
            $("#ValidTo").val('');
        }
    });

    // Download Document

    $('#displayotherdetails').on('click', '#btnDownload', function () {
        var id = $(this).data("id")
        window.location.href = "/Employees/DownloadOtherDetailsFile?detailId=" + id;
        $('.loader').addClass('d-none');
    });

    $("#btnCloseCreateOtherDetails").click(function () {
        $("#success_CreateOtherDetails_Popup").hide();
    });

    $("#btnCloseUpdateOtherDetails").click(function () {
        $("#success_UpdateOtherDetails_Popup").hide();
    });

    $("#btnCloseDeleteOtherDetails").click(function () {
        $("#success_DeleteOtherDetails_Popup").hide();
    });

</script>






























